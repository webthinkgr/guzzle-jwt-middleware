{
    "name": "webthink/guzzle-jwt",
    "homepage": "https://github.com/webthinkgr/guzzleJwt",
    "description": "This package contains useful classes and interfaces for requiring a JWT token",
    "keywords": [
        "webthink",
        "jwt",
        "guzzle"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.38",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*|^5.0",
        "psr/cache": "~1.0",
        "symfony/cache": "^3.3",
        "symfony/http-foundation": "^3.3",
        "webthink/codesniffer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webthink\\GuzzleJwt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webthink\\GuzzleJwt\\Test\\": "tests"
        }
    },
    "scripts": {
        "phpcbf": "php ./vendor/bin/phpcbf src/ tests/",
        "phpcs": "php ./vendor/bin/phpcs src/ tests/",
        "test": "php ./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
